{
  "vim_mode": true,
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_size": 14,
  "ui_font_family": "JetBrainsMono Nerd Font Mono",
  "ui_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "agent": {
    "button": true,
    "model_parameters": [],
    "default_model": {
      "provider": "google",
      "model": "gemini-2.0-flash"
    }
  },
  "terminal": { "button": false },
  "languages": {
    "Python": {
      "language_servers": ["basedpyright", "ruff", "!pyright"]
    }
  },
  "lsp": {
    "basedpyright": {
      "binary": {
        "path": "/Users/dsi-user/.asdf/installs/python/3.11.8/bin/basedpyright-langserver",
        "arguments": ["--stdio"]
      },
      "settings": {
        "python": {
          "pythonPath": ".venv/bin/python",
          "analysis": { "ignore": ["*"] }
        },
        "basedpyright.analysis": { "typeCheckingMode": "off" },
        "basedpyright.disableOrganizeImports": true
      }
    },
    "ruff": {}
  },
  "vertical_scroll_margin": 0,
  "scrollbar": { "show": "never" },
  "relative_line_numbers": true,
  "cursor_blink": false,
  "toolbar": {
    "quick_actions": false,
    "selections_menu": false,
    "breadcrumbs": false
  }
}
